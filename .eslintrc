{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "no-undef": "off",
        "no-param-reassign": "off",
        "react/require-default-props": "off",
        "react/forbid-prop-types": "off",
        "react/prop-types": "off",
        "no-shadow":"off",
        "import/prefer-default-export":"off",
        "react/sort-comp": "off"
    }
}